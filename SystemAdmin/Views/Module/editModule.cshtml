<div class="modal-dialog modal-xl">
    <div class="modal-content noBorder">
        <div class="modal-header">
            <h5 class="modal-title"><i class='fas fa-edit fa-lg'></i> Editar Módulo</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <div class="row typemodal">
                <partial name="~/Views/Module/NewModule.cshtml" />
                <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <label>Estatus:</label>
                    <br />
                    <input class="form-control inputDetail" disabled="disabled" id="lblEstatusMod" />
                </div>
                <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <label>Fecha Alta:</label>
                    <br />
                    <input class="form-control inputDetail" disabled="disabled" id="lblFechaAltaMOD" />
                </div>
                <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <label>Usuario Alta:</label>
                    <br />
                    <input class="form-control inputDetail" disabled="disabled" id="lblUsuarioAltaMod" />
                </div>
                <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <label>Fecha Mod:</label>
                    <br />
                    <input class="form-control inputDetail" disabled="disabled" id="lblFechaModMod" />
                </div>
                <div class="col-xl-4 col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <label>Usuario Mod:</label>
                    <br />
                    <input class="form-control inputDetail" disabled="disabled" id="lblUsuarioModMod" />
                </div>
            </div>

        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-danger dvCloseModalNewModal" data-dismiss="modal" onclick='$(".primerModal").removeClass("primerModal");'><i class="fas fa-window-close fa-1x" style="color:white"></i> Cancelar</button>
            <div type="submit" class="btn btn-success" onclick="updateModule();"><i class="fas fa-save fa-1x" style="color:white"></i> Guardar</div>
        </div>
    </div>
</div>
